    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
    <field name="filter_vip" label="COM_SPONSORS_FORM_LBL_PROFILE_VIP" type="list" onchange="this.form.submit();">
      <option value="0">COM_SPONSORS_PROFILES_VIP_OPTION_0</option>
      <option value="1">COM_SPONSORS_PROFILES_VIP_OPTION_1</option>
    </field>
    <field name="filter_fido" label="COM_SPONSORS_FORM_LBL_PROFILE_FIDO" type="list" onchange="this.form.submit();">
      <option value="0">COM_SPONSORS_PROFILES_FIDO_OPTION_0</option>
      <option value="1">COM_SPONSORS_PROFILES_FIDO_OPTION_1</option>
    </field>
