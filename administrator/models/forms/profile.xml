    <field name="modified_by" type="modifiedby" default="0" label="COM_SPONSORS_FORM_LBL_PROFILE_MODIFIED_BY" description="COM_SPONSORS_FORM_DESC_PROFILE_MODIFIED_BY" hidden="true" hint="COM_SPONSORS_FORM_LBL_PROFILE_MODIFIED_BY"/>
